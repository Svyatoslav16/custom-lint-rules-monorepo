{
	"name": "ilive.web.mobile",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"start": "webpack-dev-server --config devserver\\webpack\\webpack.devserver.js --open",
		"watch": "yarn build:dev",
		"build": "yarn build:prod",
		"build:dev": "webpack --config devserver\\webpack\\webpack.dev.js",
		"build:prod": "webpack --config devserver\\webpack\\webpack.prod.js",
		"vite": "vite",
		"vite:build": "vite build",
		"format": "prettier --config=.prettierrc --write **/*",
		"prepare": "cd ../.. && husky install ILive.Web.Mobile/Frontend/.husky",
		"storybook": "set NODE_OPTIONS=--openssl-legacy-provider && yarn start-storybook -p 6006",
		"storybook:build": "set NODE_OPTIONS=--openssl-legacy-provider && yarn build-storybook"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged --no-stash"
		}
	},
	"lint-staged": {
		"*.(js|jsx|ts|tsx|css|less|sass|scss|vue|json|gql|md)": [
			"prettier --config=.prettierrc --write"
		],
		"*.(js|jsx|ts|tsx)": [
			"eslint --fix"
		]
	},
